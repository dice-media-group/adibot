<%= simple_form_for(@proxy_list) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :provider_name %>
    <%= f.input :enabled %>
    <%= f.input :proxy_collection %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
