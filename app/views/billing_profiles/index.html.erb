<p id="notice"><%= notice %></p>

<h1>Billing Profiles</h1>

<table>
  <thead>
    <tr>
      <th>Nickname</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @billing_profiles.each do |billing_profile| %>
      <tr>
        <td><%= link_to billing_profile.nickname, edit_billing_profile_path(billing_profile) %></td>
        <td><%= link_to 'Destroy', billing_profile, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Add Billing Profile', new_billing_profile_path %>
